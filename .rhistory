#m=read.csv(file='d:birth.txt',header = 0,sep = '\t')
data('econ5')
m=econ5
head(m)
m=data.matrix(m)
apply(m,2,f<-function(x){kpss.test(x)$p.value})
mm=apply(m,2,f2<-function(x){diff(x,lag = 1,differences = 2)})
apply(mm,2,f<-function(x){kpss.test(x)$p.value})
mm=cbind(1:nrow(mm),mm)
nr=nrow(mm)
nrd=mm[nr,]
mm=mm[-nr,]
dim(m)
#apply(mm,2,f<-function(x){kpss.test(x)$p.value})
o=regARMA(data = mm,ar = 3,ma=3,Ndf = 2,mselection = 'BIC',with.rep = 0,iteration = 100,non.penalized = 0,method = 'alasso')
o$AR.Value
o$MA.Value
o$Betas
acf(o$Residuals);pacf(o$Residuals)
#matplot(cbind(o$y,o$Estimated.y),type='l')
abs(polyroot(c(1,-o$AR.Value)))
abs(polyroot(c(1,-o$MA.Value)))
print(sum(nrd[-c(1,2)] * o$Betas))
print(nrd[2])
matplot(abs(apply(o$coeff.trend[-c(1,2),],1,diff)),type = 'l')
matplot(mm,type='l')
acf(o$Residuals)
acf(diff(mm[,2]))
ts.plot(mm[,2])
dim(mm)
